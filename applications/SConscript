import rtconfig
from building import *

cwd     = GetCurrentDir()
CPPPATH = [cwd, str(Dir('#'))]
src     = Split("""
main.cpp
ledApps/ledblink.c
ledApps/ejdLeds.c
printerServer/printerHard.cpp
printerServer/printerServer.cpp
wifiModule/wifiServer.cpp
""")

path    = [cwd, str(Dir('#'))] 
path    += [cwd + '/ledApps']
path    += [cwd + '/printerServer']
path    += [cwd + '/wifiModule']

group = DefineGroup('Applications', src, depend = [''], CPPPATH = path)

Return('group')
