import rtconfig
from building import *

cwd     = GetCurrentDir()
CPPPATH = [cwd, str(Dir('#'))]
src     = Split("""
main.c
ledApps/ledblink.c
ledApps/ejdLeds.c
""")

path    = [cwd, str(Dir('#'))] 
path    += [cwd + '/ledApps']

group = DefineGroup('Applications', src, depend = [''], CPPPATH = path)

Return('group')
